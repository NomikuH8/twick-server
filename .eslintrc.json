{
    "env": {
        "es2021": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": "airbnb-base",
    "plugins": [
        "@typescript-eslint"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "semi": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off",
        "no-restricted-syntax": "off",
        "camelcase": "off",
        "no-continue": "off",
        "no-await-in-loop": "off"
    }
}
